<p id="notice"><%= notice %></p>

<div class="container">

  <h3 class="page-title">Compressori - S.8</h3>

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>Company</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @compressors.each do |compressor| %>
      <tr>
        <td><%= compressor.company.denominazione %></td>
        <td><%= compressor.note %></td>
        <td><%= link_to 'Show', compressor %></td>
        <td><%= link_to 'Edit', edit_compressor_path(compressor) %></td>
        <td><%= link_to 'Destroy', compressor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Crea Scheda Compressori - S.8', new_compressor_path, class: 'btn btn-energ btn-sm table-btn' %>


</div>
